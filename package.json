{
  "name": "anson-lichtfuss",
  "private": true,
  "description": "Personal website for Anson Lichtfuss",
  "version": "1.0.0-alpha.1",
  "author": "Anson Lichtfuss",
  "bugs": {
    "url": "https://github.com/ansonlichtfuss/website/issues"
  },
  "dependencies": {
    "framer-motion": "^1.10.3",
    "gatsby": "^2.21.9",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-feed": "^2.5.0",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-postcss": "^2.3.0",
    "gatsby-plugin-preact": "^3.3.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-styled-jsx": "^3.3.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-copy-linked-files": "^2.3.1",
    "gatsby-remark-images": "^3.3.0",
    "gatsby-remark-prismjs": "^3.5.0",
    "gatsby-remark-responsive-iframe": "^2.4.1",
    "gatsby-remark-smartypants": "^2.3.0",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-remark": "^2.8.4",
    "gatsby-transformer-sharp": "^2.5.0",
    "preact": "^10.4.1",
    "prismjs": "^1.20.0",
    "prop-types": "^15.7.2",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.10.0",
    "styled-jsx": "^3.2.5",
    "typeface-work-sans": "0.0.72"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^5.0.15",
    "commitizen": "^4.0.5",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.0",
    "gatsby-plugin-tslint": "0.0.2",
    "gatsby-plugin-typescript": "^2.4.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "standard-version": "^7.1.0",
    "tailwindcss": "^1.4.4",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.8.3"
  },
  "homepage": "https://github.com/ansonlichtfuss/website",
  "keywords": [
    "gatsby"
  ],
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansonlichtfuss/website.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "commit": "./node_modules/.bin/git-cz",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "chore",
        "section": "Others",
        "hidden": false
      },
      {
        "type": "revert",
        "section": "Reverts",
        "hidden": false
      },
      {
        "type": "fix",
        "section": "Bug Fixes",
        "hidden": false
      },
      {
        "type": "feat",
        "section": "Features",
        "hidden": false
      },
      {
        "type": "docs",
        "section": "Docs",
        "hidden": false
      },
      {
        "type": "style",
        "section": "Styling",
        "hidden": false
      },
      {
        "type": "refactor",
        "section": "Code Refactoring",
        "hidden": false
      },
      {
        "type": "perf",
        "section": "Performance Improvements",
        "hidden": false
      },
      {
        "type": "test",
        "section": "Tests",
        "hidden": false
      },
      {
        "type": "build",
        "section": "Build System",
        "hidden": false
      },
      {
        "type": "ci",
        "section": "CI",
        "hidden": false
      }
    ]
  }
}
