{
  "name": "anson-lichtfuss",
  "private": true,
  "description": "Personal website for Anson Lichtfuss",
  "version": "1.0.0-alpha.1",
  "author": "Anson Lichtfuss",
  "bugs": {
    "url": "https://github.com/ansonlichtfuss/website/issues"
  },
  "dependencies": {
    "@nivo/core": "^0.62.0",
    "@nivo/line": "^0.62.0",
    "@nivo/pie": "^0.62.0",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.0",
    "framer-motion": "^2.6.5",
    "gatsby": "^2.24.52",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-goatcounter": "^0.3.1",
    "gatsby-plugin-linaria": "^2.1.0",
    "gatsby-plugin-manifest": "^2.4.26",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.6.30",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-copy-linked-files": "^2.3.12",
    "gatsby-remark-images": "^3.3.28",
    "gatsby-remark-prismjs": "^3.5.11",
    "gatsby-remark-responsive-iframe": "^2.4.12",
    "gatsby-remark-smartypants": "^2.3.10",
    "gatsby-source-filesystem": "^2.3.27",
    "gatsby-transformer-remark": "^2.8.31",
    "gatsby-transformer-sharp": "^2.5.13",
    "linaria": "^1.4.0-rc.1",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "swr": "^0.3.1",
    "typeface-work-sans": "0.0.72"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@commitlint/prompt": "^9.1.2",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.11",
    "gatsby-plugin-tslint": "0.0.2",
    "gatsby-plugin-typescript": "^2.4.18",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "tailwindcss": "^1.7.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.0.2"
  },
  "homepage": "https://github.com/ansonlichtfuss/website",
  "keywords": [
    "gatsby"
  ],
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansonlichtfuss/website.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "commit": "./node_modules/.bin/git-cz",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "chore",
        "section": "Others",
        "hidden": false
      },
      {
        "type": "revert",
        "section": "Reverts",
        "hidden": false
      },
      {
        "type": "fix",
        "section": "Bug Fixes",
        "hidden": false
      },
      {
        "type": "feat",
        "section": "Features",
        "hidden": false
      },
      {
        "type": "docs",
        "section": "Docs",
        "hidden": false
      },
      {
        "type": "style",
        "section": "Styling",
        "hidden": false
      },
      {
        "type": "refactor",
        "section": "Code Refactoring",
        "hidden": false
      },
      {
        "type": "perf",
        "section": "Performance Improvements",
        "hidden": false
      },
      {
        "type": "test",
        "section": "Tests",
        "hidden": false
      },
      {
        "type": "build",
        "section": "Build System",
        "hidden": false
      },
      {
        "type": "ci",
        "section": "CI",
        "hidden": false
      }
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache",
      "prettier --write"
    ]
  }
}
